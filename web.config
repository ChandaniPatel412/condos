<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<rewrite>
			<rules>
				<rule name="static files" stopProcessing="true">
					<match url="(app/.*|css/.*|fonts/.*|images/.*|js/.*|node_modules/.*)"/>
					<conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
					<action type="None" />
				</rule>
			</rules>
		</rewrite>
	</system.webServer>
</configuration>